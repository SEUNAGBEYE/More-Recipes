{
  "name": "server",
  "version": "1.0.0",
  "description": "These is the server side of the more-recipe application",
  "main": "index.js",
  "engines": {
    "node": "8.9",
    "npm": "5.5.1"
  },
  "scripts": {
    "pretest": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && sequelize db:seed:all --env test",
    "test": "NODE_ENV=test nyc mocha ./server/src/test/**/**.spec.js --compilers js:babel-core/register --env test --exit",
    "client:side:test": "jest -u ./client/__tests__/**/** --config ./jest.config.js && codecov",
    "babel-node": "babel-node ./server/src/index.js --presets=env",
    "dev-server": "nodemon --exec npm run babel-node -- ./server/src/index.js",
    "e2e:server": "nodemon --exec npm run babel-node -- ./server/src/index.js",
    "client-server": "webpack-dev-server",
    "build": "NODE_ENV=production babel server/src -d server/dist && webpack -p --config ./webpack.prod.js",
    "build:watch": "npm run build -- --watch",
    "heroku-postbuild": "NODE_ENV=production && npm run build",
    "start": "node -- ./server/dist/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "test:all": "npm run coveralls && npm client:side:test",
    "seed": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed --seed 20171031150152-category",
    "e2e:pretest":"sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && sequelize db:seed --seed 20171031150152-category --env test",
    "e2e:setup": "selenium-standalone install",
    "e2e:test": "npm run e2e:pretest && nightwatch ./e2e-tests/index.specs.js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "server/test/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "author": "Seun Agbeye",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "cloudinary-react": "^1.0.4",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-import": "^2.8.0",
    "expect": "^22.1.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moxios": "^0.4.0",
    "node-sass": "^4.7.1",
    "nodemailer": "^4.4.2",
    "normalize.css": "^7.0.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-loader": "^2.4.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.19.0",
    "sequelize-cli": "^3.0.0",
    "sinon": "^4.3.0",
    "sqlite3": "^3.1.13",
    "style-loader": "^0.19.0",
