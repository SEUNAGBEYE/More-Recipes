    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "selenium-standalone": "^6.12.0"
  }
}
